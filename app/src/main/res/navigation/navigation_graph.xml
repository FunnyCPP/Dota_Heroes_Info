<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.v_kii_rom.dota_counterpicks.activities.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/action_mainActivity_to_hero_Info"
            app:destination="@id/hero_Info" />
    </activity>
    <activity
        android:id="@+id/hero_Info"
        android:name="com.v_kii_rom.dota_counterpicks.activities.Hero_Info"
        android:label="activity_hero__info"
        tools:layout="@layout/activity_hero__info" />
</navigation>